<div class="contentBox">
    <section class="py-2">
        <div class="container-fluid">
            <div class="row py-2">
                <div class="col-md-2"></div>
                <div class="col-sm-6  text-center">
                    <form class="form-inline" action="/space/search" method="POST">
                        <div class="input-group">
                            <input type="text" name="name"class="form-control" size="50" placeholder="Enter city or state" required>                            
                            <div class="input-group-btn">
                                <button type="submit" class="btn btn-danger">Search</button>
                            </div>
                            
                        </div>
                    </form>
                </div>
                <div class="col-md-2 text-center">
                    <div class="dropdown ml-0">
                        <button class="btn btn-info dropdown-toggle" type="button" id="dropdownFilter" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Filter
                        </button>
                        <div class="dropdown-menu" aria-labelledby="dropdownFilter">
                            <a href="/space/filter/Price(Low-High)" class="dropdown-item">Price(Low-High)</a>
                            <a href="/space/filter/Price(High-Low)" class="dropdown-item" >Price(High-Low)</a>
                            <a href="/space/filter/Volume(Low-High)" class="dropdown-item">Size(Small-Big)</a>
                            <a href="/space/filter/Volume(High-Low)" class="dropdown-item" >Size(Big-Small)</a >
                        </div>
                    </div>
                </div>
                <div class="col-md-2"></div>
            </div>
             <div class="row landing-data">
                    {{#each spaceList}}
                        <div class="col-sm-4 mb-3">
                            <div class="card">
                                <h5 class="card-header">{{this.spaceName}}</h5>
                                <div class="card-body">
                                    <div class="spaceImageCard mb-2 text-center">
                                        {{#if photoArray}}
                                            <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                                                <div class="carousel-inner">
                                                    {{#each photoArray}}
                                                        {{#if @first}}
                                                                <div class="carousel-item active">
                                                                <img class="img-fluid imgSize" src={{this}} alt="First slide">
                                                                </div>
                                                        {{else}}
                                                                <div class="carousel-item">
                                                                <img class="img-fluid imgSize" src={{this}} alt="Second slide">
                                                                </div>
                                                        {{/if}}
                                                    {{/each}} 
                                                </div>
                                                <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                                                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                                    <span class="sr-only">Previous</span>
                                                </a>
                                                <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                                                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                                    <span class="sr-only">Next</span>
                                                </a>
                                            </div> 
                                        {{else}} 
                                            
                                            <img class="img-fluid img-thumbnail imgSize" src='/public/images/noImg.png' alt="First slide">
                                            
                                        {{/if}}                      
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4 subTitle">Price</div>
                                        <div class="col-md-8">${{this.price}}/Day</div>     
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4 subTitle">Dimension</div>
                                        <div class="col-md-8">{{spaceDim.length}}ft x {{spaceDim.width}}ft x {{spaceDim.height}}ft</div>     
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4 subTitle">Address</div>
                                        <div class="col-md-8">
                                            <p class="my-0"></p>
                                            <p class="my-0"></p>
                                            <p>{{this.address.city}}, {{this.address.state}}</p>
                                        </div>     
                                    </div>
                                </div>
                                <div class="card-footer">
                                    <div class="row">
                                      
                                        <div class="col-md-3"><a href='javascript:void(0)' type='submit' data-href='/user/savedSpaces/{{this._id}}' class='favorites'>fav</a></div>
                                        
                                        <div class="col-md-5" style="text-align:right;">
                                            <p hidden class="spaceId">{{this._id}}</p>
                                            <p class="starRate" hidden>{{this.rating}}</p>                   
                                            <span class='stars'></span>
                                        </div> 
                                        <div class="col-md-3"><a type='submit' class="btn-sm btn-info"  onClick="divFunction('{{this._id}}')">Details</a></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                         {{!-- <div class="col-sm-6">
                                <div class="panel panel my-4 px-4 py-4">
                                        
                                        <div class="row py-2" onClick="divFunction('{{this._id}}')">
                                                
                                                <div class="col-sm-6 rating d-flex" >
                                                    <p class="px-2"> {{this.rating}} / 5  </p>
                                                    <p hidden class="spaceId">{{this._id}}</p>
                                                    <p class="starRate" hidden>{{this.rating}}</p>                   
                                                <span class='stars'></span>
                                            </div>
                                        </div>
                                        <div class="row"> 
                                            <div class='col-sm-8'>
                                                <div ></div>                                             
                                                <div><P>: </P></div>
                                                <div ><p>, </p></div>
                                            </div>
                                            <div class='col-sm-4 '>
                                                <a href='javascript:void(0)'  class='favorites' data-href='/user/savedSpaces/{{this._id}}'>fav</a>

                                            </div>
                                        </div>
                                </div>
                         </div> --}}
                    {{/each}}
             </div>
        </div>
    </section>
</div>  

