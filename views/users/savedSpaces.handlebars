


<div class="contentBox">
    <section class="py-2">
        <div class="container-fluid">
           <h3>Spaces of your interest</h3>
             <div class="row">
                    {{#each savedSpaces}}
                         <div class="col-sm-6">
                                <div class="panel panel my-4 px-4 py-4">
                                        <div class="row">
                                             {{#if photoArray}}
                                            <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                                                    <div class="carousel-inner">
                                                        {{#each photoArray}}
                                                            {{#if @first}}
                                                                    <div class="carousel-item active">
                                                                    <img class="d-block w-100 imgSize" src={{this}} alt="First slide">
                                                                    </div>
                                                            {{else}}
                                                                    <div class="carousel-item">
                                                                    <img class="d-block w-100 imgSize" src={{this}} alt="Second slide">
                                                                    </div>
                                                            {{/if}}
                                                        {{/each}}                      
                                                    </div>
                                                    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                                                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                                        <span class="sr-only">Previous</span>
                                                    </a>
                                                    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                                                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                                        <span class="sr-only">Next</span>
                                                    </a>
                                            </div>
                                             {{else}} 
                                            
                                            <img class="img-fluid img-thumbnail imgSize" src='/public/images/noImg.png' alt="First slide">
                                            
                                        {{/if}}           
                                        </div>
                                        <div class="row py-2" onClick="divFunction('{{this._id}}')">
                                                <div class="col-sm-6 panel-heading">
                                                    <p class='px-2'>{{this.spaceName}}</p>
                                                </div>
                                                <div class="col-sm-6 rating d-flex" >
                                                    <p class="px-2"> {{this.rating}} / 5  </p>
                                                    <p hidden class="spaceId">{{this._id}}</p>
                                                    <p class="starRate" hidden>{{this.rating}}</p>                   
                                                <span class='stars'></span>
                                            </div>
                                        </div>
                                        <div class="row"> 
                                            <div class='col-sm-8' onClick="divFunction('{{this._id}}')">
                                                <div >${{this.price}}/Night</div>                                             
                                                <div><P>Dimension: {{spaceDim.length}} x {{spaceDim.width}} x {{spaceDim.height}}</P></div>
                                                <div ><p>{{this.address.city}}, {{this.address.state}}</p></div>
                                            </div>
                                           
                                        </div>
                                         
                                </div>
                         </div>
                    {{/each}}
             </div>
        </div>
    </section>
</div>  

  